{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#root/*": ["./*"],
      "#app/*": ["./app/*"],
      "#config/*": ["./app/config/*"],
      "#utils/*": ["./app/utils/*"],
      "#plugins/*": ["./app/plugins/*"],
      "#middleware/*": ["./app/middleware/*"],
      "#controller/*": ["./app/controller/*"],
      "#model/*": ["./app/model/*"]
    }
  },
  "exclude": ["node_modules", "public"]
}
